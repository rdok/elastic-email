{
  "name": "rdok/elasticemail-php",
  "description": "Integration to the Elastic Email API http://api.elasticemail.com/public/help",
  "license": "MIT",
  "authors": [
    {
      "name": "Rizart Dokollari"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "guzzlehttp/guzzle": "^6.3",
    "phpunit/phpunit": "^6.5"
  },
  "require-dev": {
    "vlucas/phpdotenv": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "ElasticEmail\\": "src/ElasticEmail/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "ElasticEmail\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "minimum-stability": "stable",
  "scripts": {
    "phpunit": "vendor/bin/phpunit"
  }
}
